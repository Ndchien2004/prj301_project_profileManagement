<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>controller.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NhanVienServlet</servlet-name>
        <servlet-class>controller.addNhanVienServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>home</servlet-name>
        <servlet-class>controller.home</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>updateNhanVienServlet</servlet-name>
        <servlet-class>controller.updateNhanVienServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>deleteNhanVienServlet</servlet-name>
        <servlet-class>controller.deleteNhanVienServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NhanVienServlet</servlet-name>
        <url-pattern>/NhanVien</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>RegisterServlet</servlet-name>
        <servlet-class>controller.RegisterServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegisterServlet</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>home</servlet-name>
        <url-pattern>/home</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>updateNhanVienServlet</servlet-name>
        <url-pattern>/updateNhanVienServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>deleteNhanVienServlet</servlet-name>
        <url-pattern>/deleteNhanVienServlet</url-pattern>
    </servlet-mapping>
</web-app>
